a {
  color: var(--color-secondary);
  display: inline-block;
  font-weight: bold;
  text-decoration: none; }
  a:hover {
    filter: brightness(var(--hover-brightness));
    text-decoration: underline; }

blockquote {
  border-left: 5px solid var(--color);
  padding-left: 1rem; }

body {
  background: var(--color-bg-gradient);
  background-position: top;
  background-repeat: no-repeat;
  background-size: 100% 0.25rem;
  font-family: var(--font-family);
  font-size: clamp(1.125rem, calc(0.125rem + 2vw), 1.5rem);
  padding: 0.25rem 0 1.5rem; }

a b,
a strong,
button {
  background: var(--color-bg-gradient);
  border: 0;
  box-shadow: 0 0.25rem 0.25rem -0.125rem #333333aa;
  color: var(--color-bg);
  border-radius: var(--border-radius);
  display: inline-block;
  font-family: var(--font-family);
  font-size: medium;
  font-weight: bold;
  line-height: var(--line-height);
  margin: 0.5rem 0;
  padding: 1rem 2rem;
  text-shadow: 1px 1px 1px #333333aa; }
  a b:hover,
  a strong:hover,
  button:hover {
    cursor: pointer;
    filter: brightness(var(--hover-brightness)); }
  a b:active,
  a strong:active,
  button:active {
    position: relative;
    top: 0.125rem; }

/* Forms */
/* from mvp.css */
button:disabled,
input:disabled {
  background: var(--color-bg-secondary);
  border-color: var(--color-bg-secondary);
  color: var(--color-text-secondary);
  cursor: not-allowed; }

button[disabled]:hover {
  filter: none; }

form {
  border: 1px solid var(--color-bg-secondary);
  border-radius: var(--border-radius);
  display: block;
  padding: 1.5rem;
  text-align: var(--justify-normal); }

form header {
  margin: 1.5rem 0;
  padding: 1.5rem 0; }

input,
label,
select,
textarea {
  display: block;
  font-size: inherit;
  max-width: var(--width-card-wide); }

input[type="checkbox"],
input[type="radio"] {
  display: inline-block; }

input[type="checkbox"] + label,
input[type="radio"] + label {
  display: inline-block;
  font-weight: normal;
  position: relative;
  top: 1px; }

input,
select,
textarea {
  border: 1px solid var(--color-bg-secondary);
  border-radius: var(--border-radius);
  margin-bottom: 1rem;
  padding: 0.4rem 0.8rem; }

input[readonly],
textarea[readonly] {
  background-color: var(--color-bg-secondary); }

label {
  font-weight: bold;
  margin-bottom: 0.2rem;
  font-size: 0.9em; }

main h1,
main h2,
main h3,
main h4,
main h5,
main h6 {
  font-family: "Sen Extra Bold", var(--font-family);
  font-weight: normal;
  margin: 2rem 0 1rem;
  background: var(--color-bg-gradient);
  background-clip: border-box;
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  -webkit-box-decoration-break: clone;
  box-decoration-break: clone;
  text-shadow: 0 0 0 #6a6a6a2a;
  filter: drop-shadow(1px 1px #6a6a6a); }

html {
  background: var(--color-bg) url("/images/background.png");
  color: var(--color-text); }

img {
  max-width: 100%;
  height: auto;
  display: block; }

main {
  display: grid;
  grid-template-columns: var(--grid-primary); }
  @media screen and (min-width: 60rem) {
    main {
      grid-template-columns: var(--grid-desktop); } }
  main > * {
    grid-column: main-content; }
  main p {
    margin: 0; }
    main p + p {
      margin-top: 1rem; }

.banner {
  display: grid;
  grid-template-columns: var(--grid-primary);
  grid-template-rows: auto 1fr;
  background-position: bottom;
  background-repeat: no-repeat;
  background-size: cover;
  color: var(--color-text);
  font-family: "Sen Extra Bold", var(--font-family);
  margin: 1.5rem 0; }
  @media screen and (min-width: 60rem) {
    .banner {
      grid-template-columns: var(--grid-desktop); } }
  .banner > * {
    grid-column: full-content; }

.banner__tagline {
  text-align: center;
  font-size: 4rem; }

.event__image {
  height: 320px;
  border: var(--border);
  border-radius: var(--border-radius);
  box-shadow: var(--box-shadow);
  background-position: center;
  background-size: cover;
  background-repeat: no-repeat;
  width: 100%; }
  @media screen and (min-width: 60rem) {
    .event__image {
      height: 200px; } }
.event__content h3 {
  background: none;
  background-clip: unset;
  -webkit-background-clip: unset;
  -webkit-text-fill-color: var(--color-text);
  -webkit-box-decoration-break: unset;
  box-decoration-break: unset;
  line-height: 1;
  margin: 1rem 0;
  text-shadow: none;
  filter: none; }

.hero {
  display: grid;
  grid-template-rows: auto 1fr;
  align-items: center;
  background: var(--color-bg-gradient);
  border-radius: var(--border-radius);
  box-shadow: var(--box-shadow), 0 1rem 1rem -0.5rem #333333aa;
  margin: 1.5rem 0 0;
  overflow: hidden; }
  .hero svg {
    fill: #f2f2f2;
    margin: 0 auto;
    max-height: 200px;
    opacity: 0.75;
    filter: drop-shadow(1px 1px 1px #333333aa); }
  @media screen and (min-width: 60rem) {
    .hero {
      grid-template-columns: 2fr 3fr;
      grid-template-rows: auto; }
      .hero svg {
        max-height: none; } }
.hero__tagline {
  color: #f2f2f2;
  font-weight: normal;
  margin: 0;
  padding: 1rem 1rem;
  text-align: center;
  text-shadow: 1px 1px 1px #333333aa; }
  .hero__tagline strong {
    color: var(--color); }
  .hero__tagline em {
    color: var(--color-accent); }
  .hero__tagline strong,
  .hero__tagline em {
    font-weight: normal;
    font-style: normal; }

.hero__image {
  opacity: 0.5; }

.logo-icon {
  max-width: 100%;
  margin: 1rem; }

nav {
  display: grid;
  grid-template-columns: auto;
  grid-template-rows: 1fr auto;
  align-items: start; }
  nav .logo-full-icon {
    display: block;
    padding: 0.5rem; }
  @media screen and (min-width: 60rem) {
    nav {
      column-gap: 5rem;
      grid-template-columns: 1fr 1fr;
      grid-template-rows: auto; } }
.nav__list {
  list-style: none;
  margin: 0;
  padding: 0;
  text-align: right; }

.nav__item {
  display: inline-block;
  margin: 1rem; }

.nav__link {
  color: var(--color-text);
  font-weight: bold;
  text-decoration: none; }

.news {
  grid-column: full-content;
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(20rem, 1fr));
  gap: 3rem; }

.signatures {
  columns: 3;
  list-style: none;
  padding-left: 0; }
  .signatures li {
    break-inside: avoid; }
    .signatures li + li {
      margin-top: 0.5rem; }
  @media screen and (min-width: 60rem) {
    .signatures {
      grid-column: full-content;
      columns: 4; } }
.tagline {
  font-size: 5rem;
  grid-column: full-content;
  line-height: 1; }
